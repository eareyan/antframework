    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed

        try{
            /* Extraer parametros de la aplicacion */
            int iteraciones = Integer.parseInt(this.jComboBox4.getSelectedItem().toString());
            int hormigas    = Integer.parseInt(this.jComboBox5.getSelectedItem().toString());
            int nido        = this.jComboBox2.getSelectedIndex();
            int fuente      = this.jComboBox3.getSelectedIndex();
            /* Configurar y Mostrar la barra de progreso */
            this.jProgressBar1.setMaximum(iteraciones);
            this.jProgressBar1.setVisible(true);
            this.jLabel7.setVisible(true);
            /* Apagar el boton de Resolver */
            this.jButton1.setEnabled(false);
            /* Ejecutar el algoritmo seleccionado*/
            switch(this.jComboBox1.getSelectedIndex()){
                case 0:
                    AS AS = new AS(G,this,hormigas,nido,fuente,iteraciones,Double.parseDouble(this.SHEvaporacion.getText()),Double.parseDouble(this.SHAlfa.getText()),Double.parseDouble(this.SHBeta.getText()));
                    Thread TAS = new Thread(AS);
                    Thread TAS1 = new Thread(new CheckProgress(AS,TAS,this.jProgressBar1));
                    TAS.start();
                    TAS1.start();
                    break;
                case 1:
                    EAS EAS = new EAS(G,this,hormigas,nido,fuente,iteraciones,Double.parseDouble(this.SHEvaporacion.getText()),Double.parseDouble(this.SHAlfa.getText()),Double.parseDouble(this.SHBeta.getText()),Double.parseDouble(this.SHEEpsilon.getText()));
                    Thread TEAS = new Thread(EAS);
                    Thread TEAS1 = new Thread(new CheckProgress(EAS,TEAS,this.jProgressBar1));
                    TEAS.start();
                    TEAS1.start();
                    break;
                case 2:
                    RAS RAS = new RAS(G,this,hormigas,nido,fuente,iteraciones);
                    Thread TRAS = new Thread(RAS);
                    Thread TRAS1 = new Thread(new CheckProgress(RAS,TRAS,this.jProgressBar1));
                    TRAS.start();
                    TRAS1.start();
                    break;
                case 3:
                    MMAS MMAS = new MMAS(G,this,hormigas,nido,fuente,iteraciones,Double.parseDouble(this.SHEvaporacion.getText()),Double.parseDouble(this.SHAlfa.getText()),Double.parseDouble(this.SHBeta.getText()),Double.parseDouble(this.SHMMASTauMax.getText()),Double.parseDouble(this.SHMMASTauMin.getText()));
                    Thread TMMAS = new Thread(MMAS);
                    Thread TMMAS1 = new Thread(new CheckProgress(MMAS,TMMAS,this.jProgressBar1));
                    TMMAS.start();
                    TMMAS1.start();
                    break;
                case 4:
                    ACS ACS = new ACS(G,this,hormigas,nido,fuente,iteraciones,Double.parseDouble(this.ACSBeta.getText()),Double.parseDouble(this.ACSCandidatos.getText()),Double.parseDouble(this.ACSR0.getText()),Double.parseDouble(this.ACSTau.getText()),Double.parseDouble(this.ACSRo1.getText()),Double.parseDouble(this.ACSRo2.getText()));
                    Thread TACS = new Thread(ACS);
                    Thread TACS1 = new Thread(new CheckProgress(ACS,TACS,this.jProgressBar1));
                    TACS.start();
                    TACS1.start();
                    break;
                case 5:
                    ANTQ ANTQ = new ANTQ(G,this,hormigas,nido,fuente,iteraciones,Double.parseDouble(this.ACSBeta.getText()),Double.parseDouble(this.ACSCandidatos.getText()),Double.parseDouble(this.ACSR0.getText()),Double.parseDouble(this.ACSTau.getText()),Double.parseDouble(this.ACSRo1.getText()),Double.parseDouble(this.ACSRo2.getText()),Double.parseDouble(this.ANTQGamma.getText()));
                    Thread TANTQ = new Thread(ANTQ);
                    Thread TANTQ1 = new Thread(new CheckProgress(ANTQ,TANTQ,this.jProgressBar1));
                    TANTQ.start();
                    TANTQ1.start();
                    break;
            }
        }catch(Exception e){
            /* Reportar algun error en un dialog box */
            System.out.println("Error en SolveSPFrame = "+e);
            this.jButton1.setEnabled(true);
            this.jProgressBar1.setVisible(false);
            this.jLabel7.setVisible(false);
            this.reportFailure(e);

        }

    }//GEN-LAST:event_jButton1ActionPerformed

