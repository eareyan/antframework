    private Graph G;
    private String fileName;
    private DefaultTableModel modelResultados;

    /** Creates new form SolveSPFrame */
    public SolveSPFrame(Graph G,String fileName) {
        this.G = G;
        this.fileName = fileName;
        this.modelResultados = new DefaultTableModel();
        this.modelResultados.addColumn("Algoritmo");
        this.modelResultados.addColumn("Solucion");
        this.modelResultados.addColumn("f(Solucion)");
        this.modelResultados.addColumn("Velocidad (segs)");
        initComponents();
        /* Tamano de la ventana */
        this.setSize(1000, 550);
        /* Esconder algunas cosas */
        this.jProgressBar1.setVisible(false);
        this.jLabel7.setVisible(false);
        this.SHEPanel.setVisible(false);
        this.SHMMASPanel.setVisible(false);
        this.ACSPanel.setVisible(false);
        this.ANTQPanel.setVisible(false);
        /* Obtener la cantidad de nodos*/
        int cantidadNodos = this.G.getM().getRows();
        this.jTextArea1.setText(this.G.getM().toString());
        this.jLabel1.setText("<html><h4>Datos</h4><p>Grafo: "+this.fileName+"</p><p>Cantidad de Nodos:"+cantidadNodos+"</p></html>");
        this.jLabel6.setText("<html><p>Resultados para el grafo: "+this.fileName+"</p></html>");
        /* Preparar los menus con los nodos nido y fuente*/
        String [] arreglo = new String[cantidadNodos];
        for(int i=0;i<cantidadNodos;i++){
            arreglo[i] = i+"";
        }
        this.jComboBox2.setModel(new DefaultComboBoxModel(arreglo));
        this.jComboBox3.setModel(new DefaultComboBoxModel(arreglo));
    }
    public void reportResults(String algo,String result,double f,long time){
        this.jLabel7.setVisible(false);
        this.modelResultados.addRow(new Object[]{algo, result,f,time/1000.0});
        this.jTable1.setModel(modelResultados);
        this.jFrame1.setSize(410, 390);
        this.jFrame1.setVisible(true);
        this.jButton1.setEnabled(true);
    }
    public void reportFailure(Exception e){
        this.cajaError.setText(e.toString());
        this.dialogoError.setSize(450, 325);
        this.dialogoError.setVisible(true);
        
    }
